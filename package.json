{
  "name": "@uozi-admin/root",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm -F playground dev",
    "build": "tsx scripts/build.ts --parallel",
    "docs:dev": "pnpm -F docs dev",
    "docs:build": "pnpm build && pnpm -F docs build",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish",
    "lint": "eslint . --fix",
    "typecheck": "pnpm --parallel typecheck",
    "postinstall": "tsx scripts/postinstall.ts",
    "test": "pnpm -F curd test"
  },
  "dependencies": {
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@types/lodash-es": "^4.17.12",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "ora": "^9.0.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "unocss": "^66.5.4",
    "unocss-preset-chinese": "^0.3.3",
    "unocss-preset-ease": "^0.0.4",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.9",
    "vue-tsc": "^3.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild",
      "simple-git-hooks",
      "typescript",
      "vite",
      "vitest",
      "vue",
      "vue-tsc"
    ]
  }
}
