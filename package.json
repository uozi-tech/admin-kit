    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/vue": "^8.1.0",
    "@types/lodash-es": "^4.17.12",
    "chalk": "^5.6.0",
    "eslint": "^9.34.0",
    "jsdom": "^26.1.0",
    "ora": "^8.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "5.9.2",
    "unocss": "^66.5.0",
    "unocss-preset-chinese": "^0.3.3",
    "unocss-preset-ease": "^0.0.4",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild",
