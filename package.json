{
  "name": "@uozi-admin/root",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm -F playground dev",
    "build": "nx run-many --target=build --all",
    "build:config": "nx run shared-config:build",
    "build:layout": "nx run layout-antdv:build",
    "build:curd": "nx run curd:build",
