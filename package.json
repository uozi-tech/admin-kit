    "release:request": "nx release --projects request",
    "release:shared-config": "nx release --projects shared-config"
  },
  "dependencies": {
    "vue": "^3.4.37"
  },
  "devDependencies": {
    "@nx/js": "^20.0.7",
    "@vitejs/plugin-vue": "^5.1.2",
    "eslint-config-airbe": "^1.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.5.4",
    "unocss": "^0.62.4",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-define-options": "^1.5.1",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.3.0",
    "vue-tsc": "^2.0.29"
  }
}